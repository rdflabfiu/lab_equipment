      </ContextConfig>
      <ClassConfig>
         <DefaultClassConfig LogLevel="disabled"/>
         <Class Name="KRC.MDR" LogLevel="debug"/>
         <Class Name="KRC.MDR.EMI" LogLevel="debug"/>
         <Class Name="KRC.MDR.RECORD" LogLevel="debug"/>
         <Class Name="KRC.MDR.VWASCII" LogLevel="debug"/>
